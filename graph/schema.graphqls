type User {
  id: ID!
  name: String!
  email: String!
  embedding: [Float!]!
}

type AuthPayload {
  token: String!
  user: User!
}

type Query {
  users: [User!]!
  searchUsersByVector(vector: [Float!]!, topK: Int!): [User!]!
}

type Mutation {
  signup(name: String!, email: String!, password: String!, embedding: [Float!]!): User!
  signin(email: String!, password: String!): AuthPayload!
}
